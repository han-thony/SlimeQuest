        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Update menu

if (alarm[0] &lt;= 0) {
    if (obj_input.down_key) {
        audio_play_sound(snd_up_down_menu, 1, false);
        if (menu_index &lt; array_length_1d(option) - 1) {
            menu_index++;
        } else {
            menu_index = 0;
        }
        
        alarm[0] = room_speed / 6;
    }
    
    if (obj_input.up_key) {
        audio_play_sound(snd_up_down_menu, 1, false);
        if (menu_index &gt; 0) {
            menu_index--;
        } else {
            menu_index = array_length_1d(option) - 1;
        }
        
        alarm[0] = room_speed / 6;
    }
    
    if (obj_input.dash_key) {
        audio_play_sound(snd_select_menu, 1, false);
        switch (menu_index) {
            case 0:
                room_start_action = RETURN;
                room_goto(obj_stats.previous_room);
                break;
            case 1:
                scr_save_game();
                break;
            case 2:
                obj_stats.room_start_action = LOAD;
                room_goto(obj_stats.previous_room);
                break;
            case 3:
                game_end();
                break;
            case 4:
                instance_create(x,y,obj_help);
                break;
        }
        
        obj_input.dash_key = false;
    }
}
</string>
